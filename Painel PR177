  echo -e "${CYAN}[03]${NC} ğŸ•µï¸ PHISHING & Engenharia Social"
  echo -e "${CYAN}[04]${NC} ğŸ›¡ï¸ SeguranÃ§a (Rootkit, Ocultar IP)"
  echo -e "${CYAN}[05]${NC} ğŸ” Atualizar Proxies AutomÃ¡ticos"
  echo -e "${CYAN}[06]${NC} âš™ï¸ Status da Hidra (proxy/tor)"
  echo -e "${CYAN}[07]${NC} ğŸ”§ Submenu AvanÃ§ado / Oculto"
  echo -e "${CYAN}[08]${NC} ğŸ”„ Atualizar Painel Manualmente"
  echo -e "${CYAN}[09]${NC} âŒ Sair"
  echo
  read -p $'\nEscolha uma opÃ§Ã£o: ' opcao
  case $opcao in
    1) iniciar_tor_proxychains;;
    2) navegar_anonimamente;;
